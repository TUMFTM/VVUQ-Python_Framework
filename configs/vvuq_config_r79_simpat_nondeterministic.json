        },
        {
          "name": "VehicleLoad.0.mass",
          "distribution": "normal",
          "mu": 0,
          "sigma": 0.5
        }
      ]
    },
    "error_integration": {
      "method": "uncertainty_expansion"
    },
    "decision_making": {
      "confidence": 0.9,
      "qois": [
        {
          "name": "D2LL",
          "type": "absolute",
          "lower_threshold": 0,
          "upper_threshold": 1000
        },
        {
          "name": "Car.Jerk",
          "type": "absolute",
          "lower_threshold": -5,
          "upper_threshold": 5
        }
      ]
    }
  },
