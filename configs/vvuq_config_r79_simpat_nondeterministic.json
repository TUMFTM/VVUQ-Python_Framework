{
  "project": {
    "name": "SIMPAT Paper with UNECE-R79 MMU",
    "id": "SIMPAT_MMU_Paper",
    "author": "Stefan Riedmaier",
    "date": "2020-11-06",
    "description": "This project configures the non-deterministic simulation of my SIMPAT paper."
  },
  "cross_domain": {
    "simulator": {
      "passive": true,
      "simulate": true,
      "tool": "IPG_CarMaker",
      "result_folder": "data/SIMPAT_Paper/R79_nondeterministic/Simulator",
      "result_format": "erg",
      "exe": "C:/IPG/carmaker/win64-9.1.1/bin/CM.exe",
      "ip": "localhost",
      "port": 16600,
      "project": "sim",
      "testrun": "R79_LKFT_P911_1377kg"
    },
    "experiment": {
      "application": true,
      "simulate": true,
      "tool": "IPG_CarMaker",
      "result_folder": "data/SIMPAT_Paper/R79_nondeterministic/Experiment",
      "result_format": "erg",
      "exe": "C:/IPG/carmaker/win64-9.1.1/bin/CM.exe",
      "ip": "localhost",
      "port": 16600,
      "project": "sim",
      "testrun": "R79_LKFT_P911_1577kg"
    },
    "parameters": {
      "parameters": [
        {
          "name": "$Ego_Init_Velocity",
          "unit": "km/h",
          "noise": "none",
          "label": "AV Velocity"
        },
        {
          "name": "_ay_norm",
          "unit": "none",
          "noise": "none",
          "label": "Norm. Ref. Lat. Acc."
        },
        {
          "name": "$Slope",
          "unit": "Â°",
          "noise": "none",
          "label": "Road Slope"
        },
        {
          "name": "Env.Wind.Velocity",
          "unit": "km/h",
          "noise": "none",
          "label": "Wind Speed"
        },
        {
          "name": "VehicleLoad.0.mass",
          "unit": "kg",
          "noise": "none",
          "label": "Vehicle Mass"
        },
        {
          "name": "$stepsize",
          "unit": "s",
          "noise": "none",
          "label": "Step Size"
        }
      ]
    },
    "quantities": {
      "quantities": [
        {
          "name": "LatCtrl.LKAS.IsActive",
          "unit": "none",
          "filter": "none",
          "noise": "none"
        },
        {
          "name": "Car.ay",
          "unit": "m/s^2",
          "filter": "Butterworth_GRVA",
          "noise": "none"
        },
        {
          "name": "Car.v",
          "unit": "m/s",
          "filter": "none",
          "noise": "none"
        },
        {
          "name": "LatCtrl.LKAS.SwitchedOn",
          "unit": "none",
          "filter": "none",
          "noise": "none"
        },
        {
          "name": "LatCtrl.DevDist",
          "unit": "m",
          "filter": "none",
          "noise": "none"
        },
        {
          "name": "LatCtrl.DistToRight",
          "unit": "m",
          "filter": "none",
          "noise": "none"
        },
        {
          "name": "LatCtrl.DistToLeft",
          "unit": "m",
          "filter": "none",
          "noise": "none"
        },
        {
          "name": "LatCtrl.LKAS.CurveXY_trg",
          "unit": "1/m",
          "filter": "none",
          "noise": "none"
        }
      ]
    },
    "assessment": {
      "method": "UNECE-R79",
      "repetition_kpi": "none",
      "is_event_finder": false,
      "qois": [
        {
          "name": "D2LL",
          "unit": "m",
          "kpi": "min",
          "label": "Dist. to Left Line"
        },
        {
          "name": "Car.Jerk",
          "unit": "m/s^3",
          "kpi": "max",
          "label": "Ego Jerk"
        }
      ]
    }
  },
  "verification": {
    "scenarios": {
      "method": "manual",
      "space_parameters_manual": [
        {
          "name": "$Ego_Init_Velocity",
          "values": [
            100,
            100,
            100
          ]
        },
        {
          "name": "_ay_norm",
          "values": [
            0.85,
            0.85,
            0.85
          ]
        },
        {
          "name": "$Slope",
          "values": [
            0,
            0,
            0
          ]
        },
        {
          "name": "Env.Wind.Velocity",
          "values": [
            0,
            0,
            0
          ]
        },
        {
          "name": "VehicleLoad.0.mass",
          "values": [
            0,
            0,
            0
          ]
        },
        {
          "name": "$stepsize",
          "values": [
            0.001,
            0.0005,
            0.00025
          ]
        }
      ]
    },
    "discretization": {
      "discretization_method": "Richardson",
      "discretization_uncertainty": "GCI",
      "GCI_safety_factor": 1.5
    }
  },
  "calibration": {
    "method": "none"
  },
  "validation": {
    "scenarios": {
      "method": "full_factorial_doe",
      "space_parameters_ffd": [
        {
          "name": "$Ego_Init_Velocity",
          "min": 90,
          "max": 170,
          "number_samples": 3
        },
        {
          "name": "_ay_norm",
          "min": 0.4,
          "max": 0.8,
          "number_samples": 3
        },
        {
          "name": "$Slope",
          "min": -1,
          "max": 1,
          "number_samples": 2
        },
        {
          "name": "Env.Wind.Velocity",
          "min": -5,
          "max": 5,
          "number_samples": 2
        },
        {
          "name": "VehicleLoad.0.mass",
          "min": -20,
          "max": 20,
          "number_samples": 2
        }
      ]
    },
    "propagation": {
      "is_propagation": true,
      "reference_method": "single_loop",
      "reference_epistemic_method": "mcs",
      "reference_aleatory_method": "mcs",
      "reference_repetitions": 10,
      "method": "two_loop",
      "epistemic_method": "full_factorial_doe",
      "aleatory_method": "mcs",
      "number_aleatory_samples": 10,
      "epistemic_parameters": [
        {
          "name": "$Slope",
          "min": -0.1,
          "max": 0.1,
          "number_epistemic_samples": 3
        }
      ],
      "aleatory_parameters": [
        {
          "name": "$Ego_Init_Velocity",
          "distribution": "normal",
          "mu": 0,
          "sigma": 0.5
        },
        {
          "name": "_ay_norm",
          "distribution": "normal",
          "mu": 0,
          "sigma": 0.01
        },
        {
          "name": "Env.Wind.Velocity",
          "distribution": "normal",
          "mu": 0,
          "sigma": 2
        },
        {
          "name": "VehicleLoad.0.mass",
          "distribution": "normal",
          "mu": 0,
          "sigma": 0.5
        }
      ]
    },
    "metric": {
      "metric": "mavm",
      "mavm_f0": 1,
      "mavm_f1": 1
    },
    "decision_making": {
      "qois": [
        {
          "name": "D2LL",
          "type": "absolute",
          "lower_threshold": -0.3,
          "upper_threshold": 0.3
        },
        {
          "name": "Car.Jerk",
          "type": "absolute",
          "lower_threshold": -0.5,
          "upper_threshold": 0.5
        }
      ]
    },
    "error_model": {
      "method": "linear_regression",
      "prediction_interval": true,
      "alpha": 0.05,
      "extrapolation_parameters": [
        "$Ego_Init_Velocity",
        "_ay_norm",
        "$Slope",
        "VehicleLoad.0.mass"
      ]
    }
  },
  "application": {
    "scenarios": {
      "method": "full_factorial_doe",
      "space_parameters_ffd": [
        {
          "name": "$Ego_Init_Velocity",
          "min": 80,
          "max": 180,
