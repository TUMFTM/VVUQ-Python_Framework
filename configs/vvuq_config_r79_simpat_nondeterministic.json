      "method": "two_loop",
      "epistemic_method": "full_factorial_doe",
      "aleatory_method": "mcs",
      "number_aleatory_samples": 10,
      "epistemic_parameters": [
        {
          "name": "$Slope",
          "min": -0.1,
          "max": 0.1,
          "number_epistemic_samples": 3
        }
      ],
      "aleatory_parameters": [
        {
          "name": "$Ego_Init_Velocity",
          "distribution": "normal",
          "mu": 0,
          "sigma": 0.5
        },
        {
          "name": "_ay_norm",
          "distribution": "normal",
          "mu": 0,
          "sigma": 0.01
        },
        {
          "name": "Env.Wind.Velocity",
          "distribution": "normal",
          "mu": 0,
